class Animal {

   void sound() {
      System.out.println("Animal makes a sound");
   }
}

class Dog extends Animal {

   void sound() {
      System.out.println("Dog barks");
   }
}

class Goat extends Animal {

   void sound() {
      System.out.println("Goat bleats");
   }
}

class Main {
   public static void main(String[] args) {

      Animal a1 = new Dog();
      Animal a2 = new Goat();

      a1.sound();
      a2.sound();
   }
}
